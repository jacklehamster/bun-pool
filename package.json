{
  "name": "bun-pool",
  "module": "src/index.ts",
  "description": "This typescript library provides an object pool that's flexible and usable for any type of objects. It's written using bun, that's why it's called bun-pool.",
  "version": "1.0.8",
  "keywords": [
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jacklehamster/bun-pool.git"
  },
  "homepage": "https://jacklehamster.github.io/bun-pool/",
  "author": {
    "name": "Jack Le Hamster",
    "email": "jacklehamster@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "bun test",
    "build": "bun i && bun scripts/update-package.ts && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "auto-publish": "npm explore @dobuki/auto-publish -- bun run auto-publish \"$(pwd)\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dobuki/auto-publish": "^1.0.7",
    "@types/bun": "^1.0.8",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.60",
    "bun": "^1.0.29",
    "bun-types": "^1.0.29",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
